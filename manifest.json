{
  "name": "WhatsappSender",
  "description": "Send web api message to a specified number",
  "version": "0.0.2",
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
 },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon48.png"
  },
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "permissions": ["activeTab"],
  "commands": {
    "_execute_action": {
      "send_from_clipboard": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "send_from_clipboard"
    }
  }
}